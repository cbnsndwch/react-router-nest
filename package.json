{
    "name": "@cbnsndwch/react-router-7-nest-server",
    "version": "0.3.0",
    "description": "React Router 7 x NestJS custom server",
    "private": true,
    "author": {
        "name": "Sergio Leon",
        "email": "oss@cbnsndwch.io",
        "url": "https://www.cbnsndwch.io"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/cbnsndwch/react-router-7-nest-server"
    },
    "workspaces": {
        "packages": [
            "apps/*",
            "libs/*"
        ]
    },
    "scripts": {
        "clean": "turbo run clean",
        "build": "turbo run build",
        "build:web": "turbo run build --filter=\"./apps/web\"",
        "build:api": "turbo run build --filter=\"./apps/api\"",
        "build:libs": "turbo run build --filter=\"./libs/*\"",
        "format": "turbo run format",
        "lint": "turbo run lint",
        "lint:fix": "turbo run lint:fix",
        "test": "turbo run test",
        "deps": "node tools/dep-version-map.cjs && tools/dep-version-map.json",
        "clog": "conventional-changelog -p angular -i CHANGELOG.md -o changelog-temp.md && changelog-temp.md"
    },
    "devDependencies": {
        "@types/estree": "^1.0.7",
        "conventional-changelog-cli": "^5.0.0",
        "npm-check-updates": "^17.1.16",
        "prettier": "^3.5.3",
        "turbo": "^2.5.0",
        "typescript": "~5.8.3"
    },
    "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808"
}
